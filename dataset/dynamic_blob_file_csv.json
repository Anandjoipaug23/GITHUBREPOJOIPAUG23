{
	"name": "dynamic_blob_file_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_blob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().FileName,'.csv')",
					"type": "Expression"
				},
				"folderPath": "multiinput",
				"container": "copyactivity"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "C.ID",
				"type": "String"
			},
			{
				"name": "FIRST_NAME",
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}